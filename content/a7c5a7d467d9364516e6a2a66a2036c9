<script type="text/javascript"><!--
var cs = new CatalogSearch();
function onCompleteCustom(parameters) {
	var select = parameters.target;
	if (select.options.length <= 1) {
		parameters.form.submit();
		return true;
	}
	if (select.parentNode.style.display == "none") {
		select.parentNode.style.display = "";
	}
	if (parameters.level == parameters.max) {
		select.selectedIndex = 0;
	}
}
function onEmptyCustom(i, select, parameters) {
	cs.clearSelect(select);
	select.disabled = true;
	if (parameters.emptyOpt != null) {
		select.options[0] = new Option(parameters.emptyOpt.text, parameters.emptyOpt.value);
	}
	// Save selected value into hidden field for restoring
	cs.saveSelectValue(select);
	if (i > parameters.level - 1 && i > 4) {
		// hide select elements with no option, except the 3 first
		select.parentNode.style.display = "none";
	}
}
function updateCategories(parameters) {
    var selectedOption=document.getElementById("itemCategoryLevel"+(parameters.level-1)+"ID");
	if(selectedOption.value=="--"){
		selectedOption.value="";
		if(parameters.parent="--"){
			parameters.parent="";
		}
	}
	parameters.url = "/lecavalier/searchCatalogCategories.do;jsessionid=8kZLNM6GbrL8JHL4lNLYbVz8wk4SGJ4yn4VbLTLpQTGvzFjnFFFT!976219885";
	parameters.onComplete = onCompleteCustom;
	parameters.onEmpty = onEmptyCustom;
	parameters.max = 9;
	parameters.emptyOpt = new Option("--", "", true);
	cs.updateCategories(parameters);
}
addEvent(window, "load", function () {
	cs.restoreCategories({
		url: "/lecavalier/searchCatalogCategories.do;jsessionid=8kZLNM6GbrL8JHL4lNLYbVz8wk4SGJ4yn4VbLTLpQTGvzFjnFFFT!976219885",
		onComplete: onCompleteCustom,
		onEmpty: onEmptyCustom,
		min: 2,
		max: 5,
		emptyOpt: new Option("--", "", true)
	});
}, false);
//--></script>
