<script type="text/javascript"><!--
// Keep track of dynamically added Style Sheets
var dynaLoadedStyleSheets = new Array();
function openDialog() {
	var params = null;
	new SmartAjax.Request("/lecavalier/catalogShoppingCart.do;jsessionid=8kZLNM6GbrL8JHL4lNLYbVz8wk4SGJ4yn4VbLTLpQTGvzFjnFFFT!976219885", {
		onComplete: function(request, obj) {
			var dialog = document.getElementById("Dialog");
			var content = document.getElementById("DialogContent");
			
			if (request.status == 200 /*OK*/) {
				content.innerHTML = request.responseText;	/* get it as raw HTML */
				
				resizeDialog();
				addEvent(window, "resize", resizeDialog, false);
								
				if (isIE6) {
					// Hide all form controls because they are OS rendered: they appear over all elements
					toggleFormControlsVisibility("none");
				}
				
				loadStyleSheets();
				dialog.style.display = "";
			}
		}, 
		method: "get", 
		parameters: params
	}, null);
}
function closeDialog() {
	removeEvent(window, "resize", resizeDialog, false);
	removeStyleSheets();
	if (isIE6) {
		toggleFormControlsVisibility("");
	}
	document.getElementById("Dialog").style.display = "none";
}
function toggleFormControlsVisibility(value) {
	value = value == null ? "" : value;
	for (var i = 0; i < document.forms.length; i++) {
		var f = document.forms[i];
		for (var j = 0; j < f.elements.length; j++) {
			var e = f.elements[j];
			if (e.tagName == "SELECT") {
				e.style.display = value;
			}
		}
	}
}
function removeEvent(obj, evType, fn, useCapture) {
	if (obj.removeEventListener) {
		obj.removeEventListener(evType, fn, useCapture);
		return true;
	} else if (obj.detachEvent) {
		try {
			return obj.detachEvent("on"+evType, fn);
		} catch(e) {}
	} else {
		return false;
	}
}
function resizeDialog() {
	var ch = document.documentElement.clientHeight;
	document.getElementById("DialogOverlay").style.height = ch+"px";
	
	if (isIE6) {
		var opaque = document.getElementById("DialogOpaque");
		var top = ch/2;
		if (top > (opaque.clientHeight/2)) {
			opaque.style.top = top+"px"
		}
	}
}
function loadStyleSheets(sheets) {
	var head = document.getElementsByTagName("head")[0];
	var cal = findStyleSheet("jscalendar");
	var sheets = ["/css/list.css"];
	
	for (var i = 0; i < sheets.length; i++) {
		if (findStyleSheet(sheets[i]) == null) {
			var link = createStyleSheet(sheets[i]);
			head.insertBefore(link, cal);
			dynaLoadedStyleSheets.push(link);
		}
	}
}
function removeStyleSheets() {
	var head = document.getElementsByTagName("head")[0];
	while (dynaLoadedStyleSheets.length > 0) {
		head.removeChild(dynaLoadedStyleSheets.pop());
	}
}
function createStyleSheet(uri) {
	var link = document.createElement("link");
	link.media = "screen";
	link.rel = "stylesheet";
	link.href = uri;
	link.type = "text/css";
	return link;
}
function findStyleSheet(pattern) {
	var links = document.getElementsByTagName("link");
	for (var i = 0; i < links.length; i++) {
		var lk = links[i];
		if (lk.rel == "stylesheet" && lk.href && lk.href.indexOf(pattern) > -1) {
			return lk;
		}
	}
	return null;
}

function onRadioClick(fieldNum, priceTypeIDValue, stylePriceValue) {
	document.getElementById("style["+fieldNum+"].sku[0].priceTypeID").value = priceTypeIDValue;
	document.getElementById("style["+fieldNum+"].sku[0].price").value = stylePriceValue;
	document.getElementById("style["+fieldNum+"].sku[0].qty").value = 1;
	document.getElementById("updatePrice").value = "update";
	document.getElementById("styleFieldNum").value = fieldNum;
	document.getElementById("catalogShoppingCartForm").submit();
}
function removeStyle(vendorID, styleID, poID, versionNo) {
	if (confirm('Are you sure you want to delete the selected style from the order?')) {
		var sessionID = getSessionID();
		document.location.href = "/removePOStyle.do"+sessionID+"?vendorID="+
			vendorID+"&styleID="+styleID+"&poID="+poID+"&versionNo="+versionNo;
	}
}
