<script type="text/javascript">
		function MM_preloadImages() 
		   { 
		      //v3.0
		      var d=document; 
		
		      if(d.images)
		      { 
		         if(!d.MM_p)
		            d.MM_p=new Array();
		    
		         var i,j=d.MM_p.length,a=MM_preloadImages.arguments; 
		         for(i=0; i<a.length; i++)
		         {
		            if (a[i].indexOf("#")!=0)
		            {
		               d.MM_p[j]=new Image; 
		               d.MM_p[j++].src=a[i];
		            }
		         }
		      }
		   }	
			function clickSearchButton(evt){
				var btn = document.getElementById('search-submit');
				var keyCode = evt.keyCode ? evt.keyCode : evt.which ? evt.which : evt.charCode;
				if (keyCode == 13) {					
					btn.click();
					return false;					
				}
				return true;
			}
			function changeUrl(val){
				var url =document.getElementById('form1:laAdvancedsearch').href;
				if(url.indexOf("mode") > 0){
					url = url.substring(0,url.indexOf("mode")-1);
				}		
				// url = url.substring(0,);
				if(val.value =="Search inventor"){
					document.getElementById('form1:laAdvancedsearch').href = url+"?mode=inventor";
				}
				else if(val.value =="Search attorney"){
					document.getElementById('form1:laAdvancedsearch').href = url+"?mode=attorney";
				}
			}
			
			function changeAttorney(){
				document.getElementById('attorneyNImg').src='style/pbstyle/images/attorney_hover.jpg'
			}
			
			function changeInventor(){
				document.getElementById('inventorNImg').src='style/pbstyle/images/inventor_hover.jpg'
			}
		
			function backAttorney(){
				document.getElementById('attorneyNImg').src='style/pbstyle/images/attorneys_agents.jpg'
			}
			
			function backInventor(){
				document.getElementById('inventorNImg').src='style/pbstyle/images/inventors.jpg'
			}
			
		</script>
		<script type="text/javascript">
		function openTAndCPopup(){
			var tAndCWindow = window.open("termsandcondition.html","tAndC","width=550,height=280,toolbar=no,menubar=no,resizable=yes,scrollbars=yes");
			tAndCWindow.focus();

		}
		function openSearchBox(searchFor){
			if(searchFor == 'attorney'){
				document.getElementById('realSearchAttorney').style.display='block';
				document.getElementById('realSearchAttorney').focus();
				document.getElementById('tempSearchAttorney').style.display='none';
				document.getElementById('realSearchInventor').style.display='none';
				document.getElementById('tempSearchInventor').style.display='block';
			}
			else if(searchFor == 'inventor'){
				document.getElementById('realSearchInventor').style.display='block';
				document.getElementById('realSearchInventor').focus();
				document.getElementById('tempSearchInventor').style.display='none';
				document.getElementById('tempSearchAttorney').style.display='block';
				document.getElementById('realSearchAttorney').style.display='none';
			}
		}
		function clickSignIn(btn){
			document.getElementById(btn).click();
			return false;
		}	
		function pressLoginButton(evt,btn1){
			var btn = document.getElementById(btn1);
			var keyCode = evt.keyCode ? evt.keyCode : evt.which ? evt.which : evt.charCode;
			if (keyCode == 13) {					
				btn.click();
				return false;					
			}
			return true;
		}
		</script>
