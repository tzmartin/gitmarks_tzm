  <script type="text/javascript">
  var Logos = Logos ? Logos : {};
  Logos.Bible = {
    onFocus: function(el) {
      if (el.value == 'Search the Bible')
        el.value = '';
      el.className = '';
    },
    onBlur: function(el) {
      if (el.value == '')
      {
        el.value = 'Search the Bible';
        el.className = 'lbxBlurred';
      }
    }
  };

  </script>
