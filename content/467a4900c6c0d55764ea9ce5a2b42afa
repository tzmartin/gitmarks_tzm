<script type='text/javascript'>
	$(document).ready(function(){
		$('#cancel').click(function(){
			$('#recover').hide();
			$('#signin').show();
		});
		$('#forgot').click(function(){
			$('#signin').hide();
			$('#recover').show();
			return false;
		});
	});
</script>
