function toggleCategory(id, sender)
{
	list = $("category_" + id);
	if(list == null) return;
	if(list.style.display == "none")
	{
		list.style.display = "block";
		sender.innerHTML = sender.innerHTML.replace('+','-');
	}
	else 
	{
		list.style.display = "none";
		sender.innerHTML = sender.innerHTML.replace('-','+');
	}
}
</script>

