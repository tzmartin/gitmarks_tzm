<script language="javascript" type="text/javascript">

function toggleCheck(name, status, type) { 
	
	var formElements;

	if(status == 'check') { 
		$(name).descendants().each(function(item) {
			if(item.type == 'checkbox') {
				item.checked = true;
			}
		});
	} else if (status == 'uncheck') { 
		$(name).descendants().each(function(item) {
			if(item.type == 'checkbox') {
				item.checked = false;
			}
		});
	} 

}

</script>


