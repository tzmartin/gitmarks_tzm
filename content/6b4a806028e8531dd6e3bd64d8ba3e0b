	<script language="javascript" type="text/javascript">

		Event.observe(
			'search_box_form', 
			'click', 
			function() { 
				if($('search_box_form').getValue() == 'Search') {
					$('search_box_form').value = '';
				}
				
			});

		Event.observe(
			'search_box_form', 
			'mouseout', 
			function() { 

				if($('search_box_form').getValue() == '' || $('search_box_form').getValue().length == 0) { 
					$('search_box_form').value = 'Search';
				}

			});
	</script>
