<script type="text/javascript">

$(document).ready(function() {
	$('#mycarousel').jcarousel({
	});
	
	$("#android_submit").click(function() {
	  	var name = $("input#android_name").val();
		var email = $("input#android_email").val();
		var message = $("textarea#android_message").val();
		var dataString = 'name='+ name + '&email=' + email + '&message=' + message;
		$.ajax({
			type: "POST",
			url: "/android_list",
			data: dataString,
			success: function(data) {
				$('#android_list_response').html(data);
				$('#android_list_response').css('display', 'block');
			}
		});
		return false;
	});
	
	$("#yt").click(function(event) {
		event.preventDefault();
		$.nyroModalManual({
			url: $(this).attr('href')
		});
		$.nyroModalSettings({
			padding: 20
		});
	});
});

</script>
