<script type="text/javascript">
	$(document).ready(function(){
		$("#footerexpandnews").click(function(){
			$("#footermorenews").css({'display':'block'});
			$(this).css({'display':'none'});
			return false;
		});
		$("#footerhidenews").click(function(){
			$("#footermorenews").css({'display':'none'});
			$("#footerexpandnews").css({'display':'block'});
			return false;
		});
		$('#language_select form').submit(function(){
			window.location = $('#language_select form select').val();
			return false;
		});
	});
</script>
