<script type="text/javascript" charset="utf-8">
	var langPicker = {
		init: function(el){
			var all = el.children();
			all.not(".language").hide();
			el.hover(
				function() {
					all.show();
				},
				function() {
				all.not(".language").hide();
				}
			);
		}
	};
	
	$(document).ready(function() {
		langPicker.init($("#lang_picker"));
	});
	
</script>

