<script type="text/javascript">
function MailSubValidateSubmit_113447()
{
    
	document.getElementById('MailSubscribeError_113447').innerHTML = '';
	var inputId = 'IngeniMailSubscribeEmailInput_113447';

	var Input = document.getElementById(inputId);
	var val = '' + Input.value;
	var rx = /\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*/ig;
	var matches = rx.exec(val);

	if (matches != null && val == matches[0]) {
		MailSubscribe.MailSubscribeClick(113447,96287);
	}
	else {
		document.getElementById('MailSubscribeError_113447').innerHTML = 'Invalid email';
	}
}

function MailSubUnsubscribe(email)
{
	var Container = document.getElementById('IngeniMailSubscribeContainer_113447');
	Container.innerHTML = "<input id='IngeniMailSubscribeEmailInput_113447' type='hidden' value='" + email + "'/>";
	MailSubscribe.MailSubscribeClick(113447,96287);
}
